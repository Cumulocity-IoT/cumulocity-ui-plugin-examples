{
  "name": "cumulocity-ui-plugin-examples",
  "version": "2.0.3",
  "dependencies": {
    "@c8y/style": "^1004.0.0",
    "@c8y/ng1-modules": "^1004.0.0"
  },
  "scripts": {
    "serve": "c8ycli server node_modules/@c8y/ng1-modules/apps/* --env.target=targets/examples.json",
    "build": "npm run build:devicemanagement && npm run build:cockpit && npm run build:administration",
    "build:devicemanagement": "c8ycli build  node_modules/@c8y/ng1-modules/apps/devicemanagement --env.target=targets/examples.json --app.dynamicOptionsUrl=/apps/public/public-options/options.js",
    "build:cockpit": "c8ycli build  node_modules/@c8y/ng1-modules/apps/cockpit --env.target=targets/examples.json --app.dynamicOptionsUrl=/apps/public/public-options/options.js",
    "build:administration": "c8ycli build  node_modules/@c8y/ng1-modules/apps/administration --env.target=targets/examples.json --app.dynamicOptionsUrl=/apps/public/public-options/options.js",
    "deploy": "c8ycli deploy",
    "lint": "eslint --cache --",
    "test": "karma start --"
  },
  "devDependencies": {
    "@c8y/cli": "^1004.0.0",
    "@angular-devkit/build-angular": "0.8.4",
    "angular-mocks": "1.5.8",
    "babel-eslint": "6.x",
    "eslint": "3.12.2",
    "eslint-config-airbnb-es5": "1.1.0",
    "eslint-plugin-react": "6.8.0",
    "fs-extra": "1.0.0",
    "glob": "7.1.1",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-spec-reporter": "0.0.26",
    "lodash": "4.17.3",
    "sinon": "1.17.6",
    "tentacle.js": "0.1.0"
  }
}
